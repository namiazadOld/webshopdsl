shop exampleapp
{
	logo "/images/logo.png"
	title "Book Shop"
	description "Best Shop"
	sells { CD }
	
	basic product type Sellable
	{
		price : Float (summary : "true" ) 
		name : String (summary : "true" ) 
		genre : Genre{
			name : String
			desc : String
		}
	}
	
	product type Book : Sellable
	{
		publicationName : String
		publicationDate : Date (summary : "true" ) 
		author[] : Author
		{
			name : String (summary : "true" )
			age : Int

		}(summary : "true", navigable : "true") 
	}

	basic product type Gift {
		dueDate : Date (summary : "true" )
		owner 	: Owner {
			name : String
			age : Int
			
		}(summary : "true") 
	}
	product type CD : Gift
	{
		len : Int (title : "true")
		type : String (summary : "true" )
		producer : String
	}

	group by type
	group by property Sellable.genre
	group by property Gift.owner
	
	
	
}
